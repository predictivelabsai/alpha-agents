# Environment Variables for Lohusalu Capital Management
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (optional - system works in fallback mode without it)
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key (optional - for web search functionality)
TAVILY_API_KEY=your_tavily_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///./alpha_agents.db

# Application Settings
LOG_LEVEL=INFO
ENVIRONMENT=production

# Streamlit Configuration (for deployment)
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

